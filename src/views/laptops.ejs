<%- include ('partials/_header') %>
<br>

<div class="container" id="dell">
      <h1> DELL </h1>
      <div class="card-columns" id="productodell"></div>
      <script>
            var imagenesdell = [3,4,5,6,7,8,9,34,35,36,37];
            var productodell = document.getElementById('productodell');
            for(imagendell of imagenesdell){
                  productodell.innerHTML += `
                        <div class="card">    
                              <a href="#" data-toggle="modal" data-target="#id${imagendell}">
                                    <img src="img/dell/${imagendell}.png" class="card-img-top">
                              </a>
                        </div> 
                        <!-- Modal -->
                        <div class="modal fade" id="id${imagendell}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">

                              <button type="button" class="btn-close mr-2" data-dismiss="modal" aria-label="Close"></button>

                              <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
                                    <img src="img/dell/${imagendell}.png" class="img-fluid rounded"> 
                              </div>
                        </div>
                  `
            }
      </script>
</div>

<br><br>

